{
  "manifest_version": 3,
  "name": "Daily Planner Extension",
  "version": "1.0",
  "description": "A simple Daily Planner extension",
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
